<!DOCTYPE html>
<html>

<head>
	<meta charset="utf-8">
	<meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
	<meta name="description" content="Gurdeep singh osahan">
	<meta name="author" content="Gurdeep singh osahan">
	<meta name="google-site-verification" content="8R1IqiWtRxl4xmw78_KdRUQU231e7SSQg-e5lhhe9Vk" />
	<title>
		{% block title %}{{ appName }} des services pour tout vos besoins{% endblock %}
	</title>
	<!-- Favicon Icon -->
	<link rel="icon" type="{{ asset('img/Logo.png') }}" href="images/fav.svg">
	<!-- Bootstrap core CSS -->
	<link href="{{ asset('themes/base/vendor/bootstrap/css/bootstrap.min.css') }}" rel="stylesheet">
	<!-- Font Awesome-->
	<link href="{{ asset('themes/base/vendor/fontawesome/css/font-awesome.min.css') }}" rel="stylesheet">
	<!-- Material Design Icons -->
	<link href="{{ asset('themes/base/vendor/icons/css/materialdesignicons.min.css') }}" media="all" rel="stylesheet"
		type="text/css">
	<!-- Slick -->
	<link href="{{ asset('themes/base/vendor/slick-master/slick/slick.css') }}" rel="stylesheet" type="text/css">
	<!-- Lightgallery -->
	<link href="{{ asset('themes/base/vendor/lightgallery-master/dist/css/lightgallery.min.css') }}" rel="stylesheet">
	<!-- Select2 CSS -->
	<link href="{{ asset('themes/base/vendor/select2/css/select2-bootstrap.css') }}" />
	<link href="{{ asset('themes/base/vendor/select2/css/select2.min.css') }}" rel="stylesheet">
	<!-- Custom styles for this template -->
	<link href="{{ asset('themes/base/css/style.css') }}" rel="stylesheet">
	<link href="{{ asset('themes/base/css/custom.css') }}" rel="stylesheet">
	<link href="{{ asset('themes/base/css/responsive.css') }}" rel="stylesheet">
	<!-- map box -->
	<script id="search-js" defer src="https://api.mapbox.com/search-js/v1.0.0-beta.14/web.js">
	</script>

	{% block stylesheets %}
	{{ encore_entry_link_tags('app') }}
	{% endblock %}
</head>

<body data-turbo="false" class="responsive text-light"
	style="background-image: url('{{ asset('img/register.png') }}');">

	<div class="p-md-3 auth-bloc ml-auto">
		<div class="row">
			<div class="col-sm-9 mx-auto">
				<div class="pt-4">
					<a href="{{ path('accueil') }}" class="btn btn-dark shadow-sm"><i class="mdi mdi-arrow-left"></i>
						Retournez
						vers accueil</a>
				</div>
				
				{{ include('partials/_flash_message.html.twig') }}

				<div class="osahan-login py-3">
					<div class="text-center mb-4">
						<a href="{{ path('accueil') }}">
							<h5 class="text-uppercase text-light">{{ appName }}</h5>
						</a>
						<h5 class="font-weight-bold mt-3 text-light">
							{% block titre %}Inscription
							{% endblock %}
						</h5>
						<p class="text-muted"> {% block text %}{% endblock %}
						</p>
					</div>
					{% block body %}{% endblock %}

					{% block space %}
					<div style="height: 50px;" class="d-none d-sm-block"></div>
					{% endblock %}

					{% block footer %}
					<div class="py-3 d-flex align-item-center">
						<a href="{{ path('app_forgot_password_request') }}">Mot de passe oublié!</a>
						<span class="ml-auto">
							{% if app.request.attributes.get('_route') == 'app_register' %}

							<a href="{{ path('app_login') }}">Connexion</a>

							{% elseif app.request.attributes.get('_route') == 'app_login' %}

							<a href="{{ path('app_register') }}">Créer un compte</a>

							{% else %}

							<a href="{{ path('app_login') }}">Connexion</a>&nbsp;

							<a href="{{ path('app_register') }}">Créer un compte</a>

							{% endif %}
						</span>
					</div>

					<div class="py-3 text-center">
						<a class="mr-1" href="#">Besoin d'aide?</a>
					</div>

					<div class="py-3">
						<a class="text-muted mr-1" href="#">Conditons générales</a>
						<a class="text-muted mr-1" href="#">Mentions légales</a>
						<a class="text-muted mr-1" href="#">Politique de confidentialité</a>
					</div>
					{% endblock %}
				</div>
			</div>
		</div>
	</div>

	<!--       footer-->
	<!-- Bootstrap core JavaScript -->
	<script src="{{ asset('themes/base/vendor/jquery/jquery.min.js') }}"></script>
	<script src="{{ asset('themes/base/vendor/bootstrap/js/bootstrap.bundle.min.js') }}"></script>
	<script src="{{ asset('themes/base/js/dropdown.js') }}"></script>
	<script src="{{ asset('js/btnloader.js') }}"></script>
	<!-- Contact form JavaScript -->
	<!-- Do not edit these files! In order to set the email address and subject line for the contact form go to the bin/contact_me.php file. -->
	<!-- Custom -->
	<script src="{{ asset('themes/base/js/custom.js') }}"></script>

	{% block javascripts %}
	{{ encore_entry_script_tags('app') }}
	{% endblock %}

	<!-- Google tag (gtag.js) -->
	<script async src="https://www.googletagmanager.com/gtag/js?id=G-TBWPH2ZDRB"></script>
	<script>
		window.dataLayer = window.dataLayer || [];
		function gtag() { dataLayer.push(arguments); }
		gtag('js', new Date());

		gtag('config', 'G-TBWPH2ZDRB');
	</script>
</body>

</html>