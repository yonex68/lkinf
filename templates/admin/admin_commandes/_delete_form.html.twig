<form method="post" action="{{ path('app_admin_commandes_delete', {'id': commande.id}) }}" onsubmit="return confirm('Voulez-vous vraiment supprimer cette commande?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ commande.id) }}">
    <button class="btn btn-danger"><i class="mdi mdi-trash-can-outline"></i> Supprimer</button>
</form>
