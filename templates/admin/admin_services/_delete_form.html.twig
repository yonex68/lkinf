<form method="post" class="d-inline" action="{{ path('app_admin_services_delete', {'id': microservice.id}) }}" onsubmit="return confirm('Voulez vous vraiment supprimer ce service?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ microservice.id) }}">
    <button class="btn btn-danger"><i class="fa fa-trash-alt"></i> Supprimer</button>
</form>
