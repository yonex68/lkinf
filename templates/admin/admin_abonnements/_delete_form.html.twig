<form method="post" class="d-inline" action="{{ path('app_admin_abonnements_delete', {'id': abonnement.id}) }}" onsubmit="return confirm('Voulez-vous vraiment supprimer cet abonnement?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ abonnement.id) }}">
    <button class="btn btn-danger"><i class="mdi mdi-trash-can-outline"></i> Supprimer</button>
</form>
